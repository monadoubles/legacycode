{
  "name": "legacy-code-analyzer",
  "version": "1.0.0",
  "description": "Legacy Code Complexity Analyzer & Modernization Dashboard - Full Stack Next.js Application",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "node scripts/migrate.js",
    "db:seed": "node scripts/seed-data.js",
    "db:setup": "node scripts/setup-db.js",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "analyze": "ANALYZE=true npm run build",
    "migrate": "tsx src/lib/database/run-migrations.ts",
    "migrate:generate": "drizzle-kit generate:pg --out src/lib/database/migrations",
    "migrate:push": "drizzle-kit push"
